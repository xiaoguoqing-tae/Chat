page {
  height: 100%;
  background: #f4f4f4;
}
.head {
  width: 100%;
  height: 88rpx;
  text-align: center;
  position: fixed;
  top: 0;
  z-index: 100;
  line-height: 88rpx;
  border-bottom: 1px solid rgba(39, 48, 50, 0.1);
  background: rgba(244, 244, 244, 0.96);
  padding-top: 25px;
}
.head .left {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  left: 24rpx;
  bottom: 20rpx;
}
.head .right {
  width: 68rpx;
  height: 68rpx;
  position: absolute;
  right: 32rpx;
  top: 25px;
  border-radius: 20rpx;
}
.mask {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  height: 100vh;
  background: #211a4a;
  text-align: center;
}
.mask .time_second {
